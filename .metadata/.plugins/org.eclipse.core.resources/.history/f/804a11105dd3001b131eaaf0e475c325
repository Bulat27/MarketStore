package products;

import java.util.GregorianCalendar;

public class Food extends PerishableProduct{

	//It has no specific attributes or methods for now, but it's better to make seperate classes in case of future changes.
	public Food(String name, String brand, double price, GregorianCalendar expirationDate) {
		super(name, brand, price, expirationDate);
	}
	
	
	
	
	
	
}
