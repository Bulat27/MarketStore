package products;



import java.util.GregorianCalendar;

public class Food extends PerishableProduct{
	
	public Food(String name, String brand, double price, double quantity, GregorianCalendar expirationDate) {
		super(name, brand, price, quantity, expirationDate);
	}

	@Override
	public String toString() {
		return name + " - " + brand;
	}
	
	
	
	public static void main(String[] args) {
		PerishableProduct p = new PerishableProduct("sdf", "fsdf", 15 , 15, null);
		
	}
	 
	
	 
}
