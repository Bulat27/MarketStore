package products;

public class Clothes extends Product{
	
	private Size size;
	private String color;
	
	public Clothes(Size size, String color) {
		super();
		this.size = size;
		this.color = color;
	}

	public Size getSize() {
		return size;
	}

	public void setSize(Size size) {
		this.size = size;
	}

	public String getColor() {
		return color;
	}

	public void setColor(String color) {
		this.color = color;
	}
	
	
	
	
	
}
